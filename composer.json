{
  "name": "eikona-media/contao-usercentrics",
  "description": "Usercentrics Bundle for Contao Open Source CMS",
  "keywords": [
    "contao",
    "cookie",
    "consent",
    "usercentrics"
  ],
  "type": "contao-bundle",
  "homepage": "https://github.com/eikona-media/contao-usercentrics",
  "license": "MIT",
  "authors": [
    {
      "name": "eikona-media.de",
      "homepage": "https://eikona-media.de",
      "role": "Developer"
    },
    {
      "name": "Ralf Baussenwein",
      "homepage": "https://github.com/rabauss",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/eikona-media/contao-usercentrics/issues",
    "source": "https://github.com/eikona-media/contao-usercentrics"
  },
  "require": {
    "php": "^7.2 || ^8.0",
    "contao/core-bundle": "^4.9",
    "symfony/config": "4.4.* || 5.2.*",
    "symfony/dependency-injection": "4.4.* || 5.2.*",
    "symfony/http-foundation": "4.4.* || 5.2.*",
    "symfony/http-kernel": "4.4.* || 5.2.*"
  },
  "require-dev": {
    "contao/easy-coding-standard": "^3.0",
    "contao/manager-plugin": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "EikonaMedia\\Contao\\Usercentrics\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "EikonaMedia\\Contao\\Usercentrics\\ContaoManager\\Plugin"
  },
  "scripts": {
    "ecs": [
      "vendor/bin/ecs check contao/ src/ --fix --ansi",
      "vendor/bin/ecs check contao/templates --config ecs_template.php --fix --ansi"
    ]
  }
}
